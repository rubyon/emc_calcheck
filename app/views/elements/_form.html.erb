    <span class="input-group-text" style="width:150px;" id="lhv_temp">임시 저위 발열량</span>
    <%= form.text_field :lhv_temp, class: "form-control" %>
    <% element.errors.full_messages_for(:lhv_temp).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
  <!--end::Input group-->

  <div style="text-align: right;">
    <%= form.submit "저장하기", class: "btn btn-primary" %>
  </div>
<% end %>